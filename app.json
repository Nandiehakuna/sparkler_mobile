{
  "expo": {
    "name": "Sparkler",
    "slug": "sparkler_mobile",
    "version": "1.0.11",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#fff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sparkler.lol"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#fff"
      },
      "googleServicesFile": "./google-services.json",
      "package": "sparkler.lol",
      "versionCode": 3,
      "notification": {
        "icon": "./app/assets/icon.png",
        "color": "#fff"
      }
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b4c7853c-108c-4784-9190-c579d3533477"
      }
    },
    "updates": {
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/b4c7853c-108c-4784-9190-c579d3533477"
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-av",
        {
          "microphonePermission": "Sparkler would like to use your microphone for voice recording."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Sparkler would like to use your device gallery to attach images in a message."
        }
      ],
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "notification": {
      "iosDisplayInForeground": true
    }
  }
}
